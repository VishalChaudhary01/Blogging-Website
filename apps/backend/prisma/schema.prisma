
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  blogs    Blog[]
}

model Blog {
  id        Int     @id @default(autoincrement())
  authorId  Int
  content   String
  title     String
  publishDate DateTime @default(now())
  author    User    @relation(fields: [authorId], references: [id])
}